{
  "name": "@theunderscorer/utils",
  "version": "1.3.1",
  "description": "Utils for my various projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheUnderScorer/js-utils.git"
  },
  "scripts": {
    "test": "jest --runInBand",
    "build": "tsc",
    "lint": "eslint . --format=pretty --ext .ts",
    "lint:fix": "eslint . --format=pretty --ext .ts --fix",
    "version": "standard-version"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/src/**/index.ts",
      "!**/src/**/types.ts"
    ],
    "testMatch": [
      "**/src/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "utils",
    "utility",
    "helpers"
  ],
  "author": "Przemysław Żydek",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-xo": "0.29.1",
    "eslint-config-xo-typescript": "0.28.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "^24.9.0",
    "prettier": "2.0.5",
    "standard-version": "^8.0.1",
    "ts-jest": "^24.1.0",
    "typescript": "3.9.1-rc"
  }
}
