{
  "name": "@theunderscorer/utils",
  "version": "1.1.3",
  "description": "Utils for my various projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheUnderScorer/js-utils.git"
  },
  "scripts": {
    "test": "jest --runInBand",
    "build": "tsc",
    "lint": "eslint . --format=pretty --ext .ts",
    "lint:fix": "eslint . --format=pretty --ext .ts --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "utils",
    "utility",
    "helpers"
  ],
  "author": "Przemysław Żydek",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-config-xo-typescript": "^0.21.0",
    "eslint-formatter-pretty": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "typescript": "3.8.3"
  }
}
